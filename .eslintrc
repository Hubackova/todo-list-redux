{
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "react/prop-types": 1,
      "no-case-declarations": 0,
      "no-unused-vars":"warn",
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "document": true,
      "window": true,
      "localStorage": true,
      "console": true,
      "path": true,
      "__dirname": true,
      "it": true,
      "describe": true
    }
  }